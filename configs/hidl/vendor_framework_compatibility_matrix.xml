        <name>android.hardware.media.c2</name>
        <version>1.0</version>
        <interface>
            <name>IComponentStore</name>
            <instance>dolby</instance>
        </interface>
    </hal>
    <hal format="hidl">
        <name>vendor.dolby.hardware.dms</name>
        <version>2.0</version>
        <interface>
