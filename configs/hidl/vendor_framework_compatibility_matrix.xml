    <hal format="hidl">
        <name>vendor.qti.sla.service</name>
        <version>1.0</version>
        <interface>
            <name>ISlaService</name>
            <instance>default</instance>
        </interface>
    </hal>
    <hal format="hidl">
        <name>vendor.xiaomi.hw.touchfeature</name>
        <version>1.0</version>
        <interface>
            <name>ITouchFeature</name>
            <instance>default</instance>
        </interface>
